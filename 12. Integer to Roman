class Solution:
    def intToRoman(self, num: int) -> str:
        vals = [1000,900,500,400,100,90,50,40,10,9,5,4,1]
        syms = ["M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"]

        result = ""
        for i in range(len(vals)):
            count = num // vals[i] 
            result += syms[i] * count
            num -= vals[i] * count
        return result
